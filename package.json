{
  "name": "adb-qr",
  "displayName": "ADB QR",
  "description": "Connect your android device to your pc wirelessly on same network for debugging directly from vscode",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/aakashpsindhi/ADB-QR"
  },
  "icon": "icon.jpg",
  "publisher": "AakashP",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adb-qr.connect with qr",
        "title": "ADB-QR:Connect With QR Code"
      },
      {
        "command": "adb-qr.connect with pairing code",
        "title": "ADB-QR:Connect With Pairing Code"
      },
      {
        "command": "adb-qr.connect with paired device",
        "title": "ADB-QR:Connect With Paired Device"
      },
      {
        "command": "adb-qr.strem",
        "title": "ADB-QR:Streaming"
      },
      {
        "command": "ADB Qr.createTerminalHideFromUser",
        "title": "ADB-QR:Create Terminal Hide From User"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/bonjour": "^3.5.10",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/qrcode": "^1.5.0",
    "@types/vscode": "^1.64.0",
    "@types/ws": "^8.5.4",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "beamcoder": "^0.7.1",
    "bonjour": "^3.5.0",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "net": "^1.0.2",
    "qrcode": "^1.5.3",
    "vscode-languageclient": "8.1.0-next.6"
  }
}
